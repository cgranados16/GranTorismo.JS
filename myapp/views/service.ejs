<!DOCTYPE html>
<html lang="en">

<head>

  <head>
    <%- include('partials/head'); -%>
  </head>

</head>

<body>
  <header>
    <%- include('partials/header'); -%>
  </header>
  <div id="all">
    <div id="heading-breadcrumbs">
      <div class="container">
        <div class="row">
          <div class="col-md-7">
            <h1>Servicio:</h1>
          </div>
          <div class="col-md-5">
            <ul class="breadcrumb">
              <li><a href="index.html">Home</a>
              </li>
              <li><a href="portfolio-2.html">Servicios</a>
              </li>
              <li><%=serviceData.nombre%></li>
            </ul>

          </div>
        </div>
      </div>
    </div>

    <div id="content">
      <div class="container">

        <div class="row">
          <div class="col-md-12">
            <div class="heading">
              <h2><%=serviceData.nombre%></h2>
            </div>
          </div>
        </div>
        <div class="row portfolio-project">

          <section>

            <div class="col-sm-8">
              <div class="project owl-carousel">
                <%serviceData.fotos.forEach(function(item){%>
                <div class="item">
                  <img src="<%=item%>" alt="" class="img-responsive">
                </div>
                <%});%>
              </div>
              <!-- /.project owl-slider -->

            </div>

            <div class="col-sm-4">
              <div class="project-more">
                <h4>Descripci√≥n</h4>
                <p><%=serviceData.descripcion%></p>
                <h4>Preio</h4>
                <p><b>$<%=serviceData.precio%></b></p>
              </div>
            </div>

          </section>
        </div>

        <section>
          <div class="row portfolio">

            <div class="col-md-12">
              <div class="heading">
                <h3>Sugerencias</h3>
              </div>
            </div>
            <div class="col-sm-6 col-md-3">
              <%suggestions.forEach(function(suggestion){%>
              <div class="box-image">
                <div class="image">
                  <img src="<%=suggestion.fotos[0]%>" alt="texture-violet.png" class="img-responsive">
                </div>
                <div class="bg"></div>
                <div class="name">
                  <h3><a href="portfolio-detail.html"><%=suggestion.nombre%></a></h3>
                </div>
                <div class="text">
                  <p class="buttons">
                    <a href="portfolio-detail.html" class="btn btn-template-transparent-primary">View</a>
                  </p>
                </div>
              </div>
              <%});%>
              <!-- /.box-image -->
            </div>
          </div>
        </section>

      </div>
      <!-- /.container -->


    </div>
    <!-- /#content -->

    <footer>
      <%- include('partials/footer'); -%>
    </footer>

  </div>

  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
  <script>
    window.jQuery || document.write('<script src="javascripts/jquery-1.11.0.min.js"><\/script>')
  </script>
  <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/js/bootstrap.min.js"></script>

  <script src="/javascripts/jquery.cookie.js"></script>
  <script src="/javascripts/waypoints.min.js"></script>
  <script src="/javascripts/jquery.counterup.min.js"></script>
  <script src="/javascripts/jquery.parallax-1.1.3.js"></script>
  <script src="/javascripts/front.js"></script>
  <script src="/javascripts/js.cookie.js"></script>

  <!-- owl carousel -->
  <script src="/javascripts/owl.carousel.min.js"></script>
  <script src="/javascripts/signUp/signUp.js"></script>


</body>

</html>
