          <div class="panel panel-primary">
             <div class="panel-heading"><%= item.nombre%></div>
              <div class="panel-body"><%= item.descripcion%></div>
            </div>
          </div>
        </a>
        <% }); %>

        </div>



    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->

  <!-- Main Footer -->
  <footer class="main-footer">
    <%- include('partials/footer'); -%>
  </footer>
</div>
 
